PROJECTS_DIR="${P_ITERM_PROJECTS_DIR:-${HOME}/.piterm/projects}"
PROJECTS_DIR="${PROJECTS_DIR/#\~/$HOME}"

F=$(dirname $1)
D="${PROJECTS_DIR}/$F"
echo "$0, $1, $F, $D" >> ~/t
# if [[ $F != "." ]] && [[ -d  $D ]]; then
if [[ -d  $D ]]; then
	# echo "DIR $D" >> ~/t
	for f in ${D}/* ; do
		echo $F/`basename $f .sh`
	done
else 
	for f in ${PROJECTS_DIR}/* ; do
	    basename $f .sh
	done
fi
